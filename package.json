{
  "name": "mern_app",
  "version": "1.0.0",
  "description": "MERN App",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently --kill-others \"nodemon server_build\" \"cd client && npm start\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joakimolofsson/mern_app.git"
  },
  "author": "Joakim Olofsson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joakimolofsson/mern_app/issues"
  },
  "homepage": "https://github.com/joakimolofsson/mern_app#readme",
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-router": "0.0.1",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.6",
    "validator": "^10.11.0"
  },
  "proxy": "http://localhost:3001"
}
